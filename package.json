{
  "name": "valuable",
  "version": "0.2.1",
  "description": "Observable values, maps, and lists",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter spec 'test/**/*.js'",
    "cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --reporter spec 'test/**/*.js' && open coverage/lcov-report/index.html",
    "bench": "node bench/index.js",
    "test-watch": "./node_modules/.bin/mocha --watch --reporter min 'test/**/*.js'",
    "start": "cd examples/todomvc && npm run build && npm start",
    "build-debug": "browserify index.js --debug --s Valuable -o dist/valuable-debug.js",
    "build-dist": "browserify index.js --s Valuable -o dist/valuable.js",
    "build": "npm run build-debug && npm run build-dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/josephsavona/valuable.git"
  },
  "keywords": [
    "observable",
    "data",
    "model",
    "map",
    "list",
    "values"
  ],
  "author": "Joseph Savona <joe@josephsavona.com>",
  "license": "MIT License",
  "bugs": {
    "url": "https://github.com/josephsavona/valuable/issues"
  },
  "homepage": "https://github.com/josephsavona/valuable",
  "dependencies": {
    "immutable": "^2.0.3",
    "lodash": "~2.4.1",
    "moment": "^2.7.0",
    "node-uuid": "^1.4.1"
  },
  "devDependencies": {
    "backbone": "^1.1.2",
    "benchmark": "^1.0.0",
    "browserify": "^5.9.1",
    "chai": "~1.9.1",
    "istanbul": "^0.3.0",
    "jsdom": "^0.11.1",
    "mocha": "~1.20.1",
    "react": "^0.11.0",
    "sinon": "~1.10.3",
    "testling": "^1.7.0"
  },
  "testling": {
    "harness": "mocha-bdd",
    "files": "test/**/*.js",
    "browsers": [
      "ie/9..latest",
      "chrome/latest",
      "firefox/latest"
    ]
  }
}
